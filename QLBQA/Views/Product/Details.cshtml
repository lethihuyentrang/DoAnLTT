<style>
    .clamp-2 {
        display: -webkit-box;
        -webkit-box-orient: vertical;
        overflow: hidden;
        text-overflow: ellipsis;
        -webkit-line-clamp: 2;
        height: calc(2 * 1.5em);
    }

    .circle {
        width: 20px;
        height: 20px;
        border-radius: 50%;
        border: 1px solid #000;
    }

    .color-checkbox {
        display: inline-block;
        position: relative;
        padding-left: 30px;
        margin-right: 15px;
        cursor: pointer;
    }

        .color-checkbox input {
            position: absolute;
            opacity: 0;
            cursor: pointer;
        }

    .checkmark {
        position: absolute;
        top: -20px;
        left: 0;
        height: 25px;
        width: 25px !important;
        background-color: #eee;
        border-radius: 50%;
        border: 1px solid rgba(0, 0, 0, 0.3);
    }

    .color-checkbox input:checked ~ .checkmark {
        background-color: #ccc;
        box-shadow: 0 0 5px rgba(0, 0, 0, 0.3);
    }

    .checkmark:after {
        content: "";
        position: absolute;
        display: none;
    }

    .color-checkbox input:checked ~ .checkmark:after {
        display: block;
    }

    .color-checkbox .checkmark:after {
        left: 9px;
        top: 5px;
        width: 6px;
        height: 12px;
        border: solid white;
        border-width: 0 2px 2px 0;
        transform: rotate(45deg);
    }

    #size-checkmark {
        line-height: 50px;
        text-align: center;
        position: absolute;
        top: -20px;
        left: 0;
        height: 50px;
        width: 50px !important;
        background-color: #eee;
        border-radius: 8%;
        border: none;
    }

    #size-checkbox input:checked ~ .checkmark:after {
        display: none;
    }

    #size-checkbox {
        margin-right: 40px;
    }

        #size-checkbox input:checked ~ .checkmark {
            background-color: #bac34e;
            color: #fff;
        }

    .selector-wrap {
        margin-bottom: 32px;
    }
    /* CSS cho trình trượt */
    .swiper-container {
        position: relative; /* Đảm bảo swiper-container là relative để các button điều hướng có thể định vị tương đối đúng */
        width: 100%;
        height: auto;
        margin: 20px auto; /* Điều chỉnh margin tùy theo cần thiết */
    }

    .swiper-button-next,
    .swiper-button-prev {
        opacity: unset;
        visibility: unset !important;
        position: absolute; /* Đặt vị trí tuyệt đối cho các button điều hướng */
        top: 68%; /* Đặt top là 50% để nút nằm giữa trên dưới */
        transform: translateY(-50%); /* Dịch chuyển nút điều hướng lên trên một nửa chiều cao của chính nó */
        z-index: 999; /* Đảm bảo nút điều hướng luôn hiển thị trên cùng */
        color: white;
        border: none;
    }

        .swiper-button-prev:hover {
            background: none;
            color: #bac34e;
        }

        .swiper-button-next:hover {
            background: none;
            color: #bac34e;
        }

    .swiper-button-next {
        right: 10px; /* Đặt vị trí của nút next */
    }

    .swiper-button-prev {
        left: 10px; /* Đặt vị trí của nút prev */
    }

    .thumb-img {
        cursor: pointer; /* Biến con trỏ thành kiểu tay nhấn khi di chuột vào ảnh */
    }

    .single-product-content > .title {
        margin-top: 28px;
        font-size: 40px;
    }

    .price-box .new-price {
        font-size: 28px;
        font-weight: bold;
    }

    .price-box .old-price {
        font-size: 16px;
    }

    .btn-custom-size.lg-size {
        width: 260px;
    }

    .quantity-with-btn {
        margin-top: 40px;
    }

    .btn-custom-size {
        width: 182px;
    }

    .breadcrumb-height {
        height: 260px;
    }
    /* CSS cho bảng */
    /* Style for the table */
    /* Định dạng phần tử bảng */
    .selector-wrap table {
        width: 100%;
        border-collapse: collapse;
    }

    /* Định dạng tiêu đề */
    .selector-wrap th {
        background-color: #f2f2f2;
        border: 1px solid #ddd;
        padding: 8px;
    }

    /* Định dạng hàng */
    .selector-wrap td {
        border: 1px solid #ddd;
        padding: 8px;
    }

    /* Định dạng hàng chẵn */
    .selector-wrap tr:nth-child(even) {
        background-color: #f2f2f2;
    }

    .single-product-content .quantity-with-btn li:not(:last-child) {
        padding-right: 42px;
